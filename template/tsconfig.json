{
  "compileOnSave": false,
  "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "lib": [
          "dom",
          "DOM.Iterable",
          "ES2022"
      ],
      "jsx": "preserve",
      "target": "ES2022",
      "module": "ESNext",
      "moduleResolution": "node",
      "baseUrl": ".",
      "noImplicitAny": false,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "resolveJsonModule": true,
      "removeComments": false,
      "preserveConstEnums": true,
      "sourceMap": true,
      "skipLibCheck": true
  }
}
